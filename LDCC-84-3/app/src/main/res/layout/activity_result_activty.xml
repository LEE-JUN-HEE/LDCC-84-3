<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/seveneleven1"
    android:orientation="vertical"
    tools:context="com.example.juni.ldcc_84_3.ResultActivty">

    <TextView
        android:id="@+id/resulttext"
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:gravity="center"
        android:text="내가 결과를 들고왔소"
        android:textAlignment="center"
        android:textColor="@android:color/darker_gray"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/resultad"
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:layout_weight="3"
        app:srcCompat="@drawable/pokemon1" />

    <include
        android:id="@+id/item1"
        layout="@layout/final_result"></include>

    <include
        android:id="@+id/item2"
        layout="@layout/final_result"></include>

    <include layout="@layout/final_result" android:id = "@+id/item3"></include>
